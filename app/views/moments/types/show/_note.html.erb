<% moment ||= @moment %>
<% hide ||= false %>
<div class="<%= moment.theme %>" <%= "style=display:none" if hide %> >
  <div class="note">
    <div class="date">
      <%= moment.full_time.strftime "%Y-%m-%d  %H:%m" %>
    </div>
    <div class="content">
      <%= moment.note.body %>
    </div>
  </div>
</div>
