<% unless @empty %>
  <% content_for :js do %>
    <%= javascript_include_tag 'jquery.timer' %>
    <%= javascript_include_tag :moments_wall %>
  <% end %>
  <% content_for :css do %>
    <%= stylesheet_link_tag :moments %>
  <% end %>
<% end %>

<div class="container">
  <% if @empty %>
    <p>You have no moment, why not to create one? <%= link_to t("views.moment.titles.new"), new_moment_path %></p>
  <% else %>
    <div id="wall">
    </div>
  <% end %>
</div>
